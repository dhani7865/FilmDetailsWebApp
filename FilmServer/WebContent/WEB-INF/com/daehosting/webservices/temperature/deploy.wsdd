<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TemperatureConversions WSDL service -->

  <service name="TemperatureConversionsSoap" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://webservices.daehosting.com/temperature"/>
      <parameter name="wsdlServiceElement" value="TemperatureConversions"/>
      <parameter name="schemaQualified" value="http://webservices.daehosting.com/temperature"/>
      <parameter name="wsdlServicePort" value="TemperatureConversionsSoap"/>
      <parameter name="className" value="com.daehosting.webservices.temperature.TemperatureConversionsSoapBindingSkeleton"/>
      <parameter name="wsdlPortType" value="TemperatureConversionsSoapType"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <parameter name="allowedMethods" value="*"/>

  </service>
</deployment>
